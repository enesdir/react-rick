{
	"name": "react-select-light-example",
	"version": "0.1.1",
	"type": "module",
	"description": "Usage of react-select-light with pokeman api",
	"author": "enesdir",
	"private": true,
	"engines": {
		"node": ">=18.x.x",
		"npm": ">=9"
	},
	"keywords": [
		"react",
		"reactjs",
		"ui",
		"select",
		"pokeman",
		"multiselect"
	],
	"scripts": {
		"build": "vite build",
		"build:turbo": "npx turbo build",
		"check:turbo": "npx turbo lint type-check",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint src",
		"lint-staged": "lint-staged",
		"lint:fix": "eslint . --fix --cache && npm run format",
		"prepare": "husky install",
		"push-release": "git push --follow-tags origin main",
		"release": "dotenv -e HUSKY=0 commit-and-tag-version",
		"serve": "vite preview",
		"type-check": "tsc --noEmit"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"dependencies": {
		"million": "2.6.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-select-light": "*"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.4",
		"@commitlint/config-conventional": "^18.4.4",
		"@commitlint/cz-commitlint": "^18.4.4",
		"@commitlint/prompt-cli": "^18.4.4",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.1",
		"@types/node": "^20.10.6",
		"@types/react": "18.2.46",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"commit-and-tag-version": "^12.0.0",
		"commitizen": "^4.3.0",
		"commitlint": "^18.4.4",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"inquirer": "^8.2.6",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prettier-plugin-pkg": "^0.18.0",
		"prettier-plugin-sh": "^0.13.1",
		"stylelint": "^16.1.0",
		"stylelint-config-standard": "^36.0.0",
		"turbo": "1.11.3",
		"typescript": "5.3.3",
		"vite": "^5.0.10",
		"vite-plugin-inspect": "^0.8.1",
		"vite-plugin-pwa": "^0.17.4"
	}
}
